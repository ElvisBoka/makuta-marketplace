{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "DATABASE_URL": "@makutaplace-database-url",
    "JWT_SECRET": "@makutaplace-jwt-secret",
    "JWT_EXPIRE": "30d",
    "NODE_ENV": "production"
  }
}